{
  "AppName": "CsvTools",
  "Version": "1.0.0",
  "Environment": "Development",
  "CsvSettings": {
    "InputDirectory": "./input",
    "OutputDirectory": "./output",
    "FileEncoding": "UTF-8",
    "Delimiter": ",",
    "HasHeader": true,
    "BatchSize": 1000,
    "DefaultLangId": "cn",
    "LangId": "cn"
  },
  "Database": {
    "ConnectionString": "Server=localhost;Database=test;Uid=user;Pwd=password;",
    "Timeout": 30
  },
  "Performance": {
    "MaxConcurrentFiles": 5,
    "MemoryThresholdMB": 512
  }
}